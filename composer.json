{
    "name": "fire-fly/filament-blog",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FireFly\\FilamentBlog\\": "src/",
            "FireFly\\FilamentBlog\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FireFly\\FilamentBlog\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Asmit Nepali"
        }
    ],
    "require": {
        "filament/support": "^3.2",
        "awcodes/filament-tiptap-editor": "3.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FireFly\\FilamentBlog\\FilamentBlogServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9",
        "pestphp/pest": "^2",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
